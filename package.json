{
  "name": "@alfredvaa/react-components-boilerplate",
  "author": "Alfred Yrelin",
  "version": "0.0.6",
  "main": "build/index.js",
  "module": "build/index.es.js",
  "types": "build/index.d.ts",
  "repository": "git://github.com/alfredvaa/react-components-boilerplate.git",
  "files": [
    "build"
  ],
  "scripts": {
    "start": "start-storybook",
    "build": "npm run build-lib && build-storybook",
    "clean": "run() { cd src/$1 && rimraf build; }; run ",
    "build-lib": "npm run lint && rollup -c",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test:coverage": "npm test -- --coverage",
    "test:update": "npm test -- --u --coverage",
    "build-storybook": "-c .storybook -o .out",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "version-module": "run() { yarn clean $1 && yarn build-lib && version=$(npm version --no-git-tag-version $2) && (cd src/$1 && npm version $version && npm publish) && git add package.json && git add src/$1/package.json && git commit -m $version && git tag $version -am $version && git push --follow-tags; }; run",
    "patch": "run() { yarn version-module $1 patch; }; run",
    "minor": "run() { yarn version-module $1 minor; }; run",
    "major": "run() { yarn version-module $1 major; }; run"
  },
  "dependencies": {
    "@storybook/addon-actions": "^5.0.6",
    "@storybook/addon-info": "^5.0.6",
    "@storybook/addon-knobs": "^5.0.6",
    "@storybook/addon-links": "^5.0.6",
    "@storybook/addon-options": "^5.0.6",
    "@storybook/react": "^5.0.6",
    "react-transition-group": "^4.1.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "peerDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "rollup": "^1.14.6",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.0.0",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^5.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.21.1",
    "rollup-plugin-url": "^2.2.1",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "@types/react-transition-group": "^2.9.2",
    "@babel/core": "^7.4.5",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.13.2",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook-addon-jsx": "^5.4.3",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-info": "^4.1.1",
    "@types/storybook__addon-knobs": "^4.0.5",
    "@types/storybook__addon-links": "^3.3.4",
    "@types/storybook__react": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.4",
    "cpx": "^1.5.0",
    "css-loader": "^2.1.1",
    "jest": "^24.7.1",
    "npm": "^6.13.4",
    "react": "^16.8.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.6",
    "react-test-renderer": "^16.8.6",
    "style-loader": "^0.23.1",
    "tachyons": "^4.10.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.15.0",
    "typescript": "^3.4.2",
    "webpack": "^4.33.0"
  }
}
